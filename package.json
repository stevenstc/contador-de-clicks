{
  "name": "contador-de-clicks",
  "displayName": "Contador de Clicks",
  "version": "1.3.4",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "homepage": "./",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npx rimraf www && npx move-cli build www",
    "apk": "react-scripts build && npx rimraf www && npx move-cli build www && cordova build android",
    "aab": "react-scripts build && npx rimraf www && npx move-cli build www && cordova build android -release",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "stc Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "cordova-android": "^12.0.0",
    "cordova-browser": "^6.0.0",
    "rimraf": "^4.1.2"
  },
  "cordova": {
    "platforms": [
      "browser",
      "android"
    ]
  }
}
